<Window x:Class="SeriesTracker.Windows.WindowLoggingIn"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:ViewModels="clr-namespace:SeriesTracker.ViewModels"
		DataContext="{Binding LoggingInViewModel, Source={StaticResource Locator}}"
		Style="{StaticResource STWindowNoBorder}"
		Height="100" Width="300"
		Loaded="Window_Loaded" MouseDown="Window_MouseDown"
		WindowStartupLocation="CenterScreen">
	<Grid>
		<Label x:Name="lbl_text"  Content="{Binding LoginText}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
	</Grid>
</Window>
