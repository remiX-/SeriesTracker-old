<Window
	x:Class="SeriesTracker.Windows.WindowLoggingIn"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:ViewModels="clr-namespace:SeriesTracker.ViewModels"
	Height="100" Width="300"
	Loaded="Window_Loaded" MouseDown="Window_MouseDown"
	WindowStartupLocation="CenterScreen"
	Style="{StaticResource ReWindowNoBorder}">
	<Window.DataContext>
		<ViewModels:ViewLoggingInViewModel/>
	</Window.DataContext>
	<Grid>
		<Label x:Name="lbl_text"  Content="{Binding LoginText}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
	</Grid>
</Window>
