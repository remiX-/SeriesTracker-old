<Page
	x:Class="SeriesTracker.Views.PageTwo"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	Foreground="Black"
	mc:Ignorable="d" 
	d:DesignHeight="450"
	d:DesignWidth="800"
	Title="Page Two">
	<Grid Background="{DynamicResource AccentColorBrush4}">
		<TextBlock Text="Welcome to Page Two" Margin="24" FontSize="48" VerticalAlignment="Top" HorizontalAlignment="Left" />
		<TextBlock Text="{Binding MyTitle}" Margin="24" FontSize="48" VerticalAlignment="Bottom" HorizontalAlignment="Left" />
	</Grid>
</Page>